                        <Insets />
                     </HBox.margin>
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                     <styleClass>
                        <String fx:value="scroll-bar-List" />
                        <String fx:value="event-list" />
                        <String fx:value="list-cell" />
                        <String fx:value="bg-color-main" />
                     </styleClass></ListView>
               </children>
            </HBox>
            <Separator opacity="0.0" prefHeight="10.0" prefWidth="200.0" />
         </children>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="bg-color-main" />
      <String fx:value="set-Display" />
