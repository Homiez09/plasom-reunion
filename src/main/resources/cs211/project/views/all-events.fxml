               </children>
            </HBox>
            <Separator opacity="0.0" prefHeight="3.0" prefWidth="200.0" />
            <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="1152.0">
               <children>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="33.0" prefWidth="63.0" />
                  <StackPane prefHeight="42.0" prefWidth="90.0">
                     <children>
                        <ComboBox fx:id="categoryComboBox" prefWidth="65.0" />
                        <Button mnemonicParsing="false" onAction="#onCategory" prefWidth="65.0" text="Category" />
                     </children>
                  </StackPane>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="33.0" prefWidth="12.0" />
                  <Label fx:id="tagComboBox" onMouseClicked="#onClickTagLabel" text="Label">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="42.0" prefWidth="135.0" />
                  <TextField fx:id="searchbarTextField" prefHeight="25.0" prefWidth="500.0" />
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="42.0" prefWidth="120.0" />
                  <StackPane>
                     <children>
                        <ImageView fx:id="sortImageView" fitHeight="30.0" fitWidth="30.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../images/hostevent/sort.png" />
                           </image>
                        </ImageView>
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" style="-fx-background-color: transparent;" styleClass="hide-button" />
                     </children>
                  </StackPane>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="33.0" prefWidth="10.0" />
                  <Button mnemonicParsing="false" onAction="#onCreateClick" prefHeight="25.0" prefWidth="100.0" text="+ Create Event" />
               </children>
            </HBox>
            <Separator opacity="0.0" prefHeight="3.0" prefWidth="200.0" />
