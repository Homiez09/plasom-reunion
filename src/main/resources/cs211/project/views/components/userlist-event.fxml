<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs211.project.componentControllers.UserListEventController">
   <children>
      <AnchorPane>
         <children>
            <VBox>
               <children>
                  <HBox alignment="CENTER_LEFT">
                     <children>
                        <Separator mouseTransparent="true" opacity="0.0" orientation="VERTICAL" prefHeight="50.0" prefWidth="25.0" style="-fx-border-color: #FFFFF;" />
                        <Label fx:id="eventNameLabel" prefHeight="35.0" prefWidth="330.0" text="Event" textOverrun="CENTER_WORD_ELLIPSIS">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Separator mouseTransparent="true" opacity="0.0" orientation="VERTICAL" prefWidth="25.0" style="-fx-border-color: #FFFFF;" />
                        <Label fx:id="eventNameLabel" alignment="CENTER" text="Users " textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Separator mouseTransparent="true" opacity="0.0" orientation="VERTICAL" prefWidth="10.0" style="-fx-border-color: #FFFFF;" />
                        <Label fx:id="userSizeLabel" alignment="CENTER" text="size" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Separator mouseTransparent="true" opacity="0.0" orientation="VERTICAL" style="-fx-border-color: #FFFFF;" />
                        <Label fx:id="eventNameLabel11" alignment="CENTER" text="total" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Separator mouseTransparent="true" opacity="0.0" orientation="VERTICAL" prefHeight="27.0" prefWidth="356.0" style="-fx-border-color: #FFFFF;" />
                        <StackPane>
                           <children>
                              <Button fx:id="openButton" mnemonicParsing="false" onAction="#onOpenAction" prefWidth="50.0" text="Open" />
                              <Button fx:id="closeButton" mnemonicParsing="false" onAction="#onCloseAction" prefWidth="50.0" text="Close" />
                           </children>
                        </StackPane>
                     </children>
                  </HBox>
                  <Separator prefHeight="16.0" />
                  <TableView fx:id="TableEvent">
                     <columns>
                        <TableColumn fx:id="checkboxColumn" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" sortable="false" text="Select" />
                        <TableColumn fx:id="profileColumn" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" sortable="false" text="Profile" />
                        <TableColumn fx:id="usernameColumn" editable="false" maxWidth="125.0" minWidth="125.0" prefWidth="125.0" sortable="false" text="Username" />
                        <TableColumn fx:id="nameColumn" editable="false" maxWidth="125.0" minWidth="125.0" prefWidth="125.0" sortable="false" text="Name" />
                        <TableColumn fx:id="statusColumn" editable="false" maxWidth="130.0" minWidth="130.0" prefWidth="130.0" sortable="false" text="Status" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <Separator opacity="0.0" prefHeight="15.0" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</VBox>
